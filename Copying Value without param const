package com;

class Demo{
	int id;
	String name;
	
	Demo(int i,String n)
	{
		id=i;
		name=n;
	}
	Demo(){
		
	}
	void disp()
	{
		System.out.println(id+" "+name);
	}
	public static void main(String[]args)
	{
	Demo d=new Demo(12,"Ankit");
	d.disp();
	
    Demo d2=new Demo();
	d2.id=d.id;
	d2.name=d.name;
	d2.disp();
		
