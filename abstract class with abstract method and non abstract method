package com;

abstract class Bike
{
	Bike()
	{
		
		System.out.println("Bike is Start..");
	}
	abstract void run();
	void gearChange()
	{
		System.out.println("Gear Changed..");
	}
}
class Shine extends Bike
{
	void run()
	{
		System.out.println("Bike is Running..");
	}
}

class Demo
{	
	public static void main (String []args)
	{
 
		Bike b=new Shine();
		b.run();
		b.gearChange();
		
		
	}
}
