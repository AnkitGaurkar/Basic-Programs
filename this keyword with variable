package com;

class Demo{
	int id;
	String name;
	float salary;
	
	Demo(int id,String name,float salary)
	{
		this.id=id;
		this.name=name;
		this.salary=salary;
	}
	void disp()
	{
		System.out.println(id+" "+name+" "+salary);
	}
	
	public static void main (String []args)
	{
	
		Demo d=new Demo(2, "Ankit", 23345);
		d.disp();
		Demo d2=new Demo(4, "Rohan", 4444);
		d2.disp();
}
}
