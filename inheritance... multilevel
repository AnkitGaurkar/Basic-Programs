package com;

class Animal{
	void eat()
	{
		System.out.println("Eating...");
	}
	
}
class Dog extends Animal 
{
	void bark()
	{
		System.out.println("Barking...");
	}
}
class VoiceDog extends Dog
{
	void voice()
	{
		System.out.println("Bhook Bhook...");
	}
}

class Demo
{
	public static void main (String []args)
	{
	VoiceDog d=new VoiceDog();
	d.bark();
	d.eat();
	d.voice();
}
}
